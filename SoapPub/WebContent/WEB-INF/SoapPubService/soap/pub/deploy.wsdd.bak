<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SoapPubService WSDL service -->

  <service name="SoapPub" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://pub.soap"/>
      <parameter name="wsdlServiceElement" value="SoapPubService"/>
      <parameter name="schemaQualified" value="http://pub.soap"/>
      <parameter name="wsdlServicePort" value="SoapPub"/>
      <parameter name="className" value="soap.pub.SoapPubSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="SoapPub"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="infoPublication" qname="operNS:infoPublication" xmlns:operNS="http://pub.soap" returnQName="retNS:infoPublicationReturn" xmlns:retNS="http://pub.soap" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:title" xmlns:pns="http://pub.soap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getAllPub" qname="operNS:getAllPub" xmlns:operNS="http://pub.soap" returnQName="retNS:getAllPubReturn" xmlns:retNS="http://pub.soap" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="pubFromRes" qname="operNS:pubFromRes" xmlns:operNS="http://pub.soap" returnQName="retNS:pubFromResReturn" xmlns:retNS="http://pub.soap" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://pub.soap" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getAllPub infoPublication pubFromRes"/>

  </service>
</deployment>
